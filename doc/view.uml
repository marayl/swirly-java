@startuml

skinparam monochrome true

class Ladder {
  bidTicks: long[N]
  bitLots: long[N]
  bidCount: int
  offerTicks: long[N]
  offerLots: long[N]
  offerCount: int
}

class View {
  market: String
  contr: String
  settlDay: int
  ladder: Ladder
  lastTicks: long
  lastLots: long
  lastTime: long
}

Ladder <-- View

@enduml
