@startuml

skinparam monochrome true

abstract class Rec {
  mnem: String
  display: String
}

class Asset {
  type: AssetType
}

class Contr {
  asset: Memorable
  ccy: Memorable
  tickNumer: int
  tickDenom: int
  lotNumer: int
  lotDenom: int
  pipDp: int
  minLots: long
  maxLots: long
}

class Market {
  contr: Memorable
  settlDay: int
  expiryDay: int
}

class Trader {
  email: String
}

Rec <|-- Asset
Rec <|-- Contr
Rec <|-- Market
Rec <|-- Trader

@enduml
