<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_5.xsd" version="5">
  <!-- Modules -->

  <!-- App Engine will ignore the <context-root> elements, so HTTP clients need not prepend it to
       the URL path when addressing a module. -->
  <display-name>swirly-prod</display-name>
  <module>
    <!-- The default module must be listed first. -->
    <web>
      <web-uri>swirly-front-0.1</web-uri>
      <context-root>default</context-root>
    </web>
    <web>
      <web-uri>swirly-back-0.1</web-uri>
      <context-root>backend</context-root>
    </web>
  </module>
</application>
